function buildRules({ quicEnabled }) {
  const ruleList = [...baseRules];
  if (!quicEnabled) {
    ruleList.unshift("AND,((NETWORK,UDP),(DST-PORT,443)),REJECT");
    ruleList.unshift("AND,((NETWORK,UDP),(DST-PORT,8443)),REJECT");
  }
  return ruleList;
}

