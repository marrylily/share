function getCountryGroupNames(countryInfo, minCount) {
  return countryInfo
    .filter((item) => item.count >= minCount)
    .map((item) => `${countriesMeta[item.country].flag} ${item.country}${NODE_SUFFIX}`);
}

function stripNodeSuffix(groupNames) {
  const suffixPattern = new RegExp(`${NODE_SUFFIX}$`);
  return groupNames.map((name) => {
    const raw = name.replace(suffixPattern, "");
    return raw.replace(/^..\s+/, "").trim();
  });
}

const PROXY_GROUPS = {
  SELECT: "é€‰æ‹©ä»£ç†",
  MANUAL: "æ‰‹åŠ¨é€‰æ‹©",
  FALLBACK: "æ•…éšœè½¬ç§»",
  DIRECT: "ç›´è¿ž",
  LANDING: "è½åœ°èŠ‚ç‚¹",
  LOW_COST: "ä½Žå€çŽ‡èŠ‚ç‚¹",
  AUTO: "è‡ªåŠ¨é€‰æ‹©",
};

const buildList = (...elements) => elements.flat().filter(Boolean);

/**
 * âœ… æœ€ç»ˆç¨³å®šå…œåº•ç­–ç•¥ï¼š
 * - é€‰æ‹©ä»£ç†(select) ä»ç„¶æŠŠ fallback æ”¾ç¬¬ä¸€ä½ï¼ˆä¹ æƒ¯ä¸å˜ï¼‰
 * - æ•…éšœè½¬ç§»(fallback) ä¸å†åŒ…å« selectï¼ˆé¿å… loopï¼‰
 * - fallback é‡ŒåŠ å…¥ AUTOï¼ˆå¢žå¼ºå…œåº•èƒ½åŠ›ï¼‰
 */
function buildBaseLists({ landing, lowCost, countryGroupNames }) {
  // é€‰æ‹©ä»£ç†ï¼ˆå…¥å£ï¼‰
  const defaultSelector = buildList(
    PROXY_GROUPS.FALLBACK,
    landing && PROXY_GROUPS.LANDING,
    countryGroupNames,
    lowCost && PROXY_GROUPS.LOW_COST,
    PROXY_GROUPS.MANUAL,
    "DIRECT"
  );

  const defaultProxies = buildList(
    PROXY_GROUPS.SELECT,
    countryGroupNames,
    lowCost && PROXY_GROUPS.LOW_COST,
    PROXY_GROUPS.MANUAL,
    PROXY_GROUPS.DIRECT
  );

  const defaultProxiesDirect = buildList(
    PROXY_GROUPS.DIRECT,
    countryGroupNames,
    lowCost && PROXY_GROUPS.LOW_COST,
    PROXY_GROUPS.SELECT,
    PROXY_GROUPS.MANUAL
  );

  // âœ… å…³é”®ï¼šfallback ä¸å†åŒ…å« selectï¼Œä½†åŠ å…¥ AUTOï¼ˆå…œåº•æ›´å¼ºï¼‰
  const defaultFallback = buildList(
    landing && PROXY_GROUPS.LANDING,
    countryGroupNames,
    PROXY_GROUPS.AUTO,          // âœ… æ–°å¢žï¼šæ™ºèƒ½å…œåº•æµ‹é€Ÿ
    lowCost && PROXY_GROUPS.LOW_COST,
    PROXY_GROUPS.MANUAL,
    "DIRECT"
  );

  return { defaultProxies, defaultProxiesDirect, defaultSelector, defaultFallback };
}

